{
    "name": "mrjuliuss/lecter",
    "description": "Lecter, a flat file wiki built with Laravel 5",
    "keywords": ["laravel","flat","markdown", "wiki"],
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Richarte",
            "email": "julien.richarte@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "erusev/parsedown-extra": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "MrJuliuss\\Lecter\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}